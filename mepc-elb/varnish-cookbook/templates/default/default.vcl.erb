backend <%= node['varnish']['backend_jenkins_name'] %> {
        .host = "<%= node['varnish']['backend_jenkins_host'] %>";
        .port = "<%= node['varnish']['backend_jenkins_port'] %>";
}
sub vcl_recv {
	if (req.url ~ "^/<%= node['varnish']['backend_jenkins_url'] %>") {
		set req.backend = <%= node['varnish']['backend_jenkins_name'] %>;
	} else {
		error 404 "Not Found";
	}
}
